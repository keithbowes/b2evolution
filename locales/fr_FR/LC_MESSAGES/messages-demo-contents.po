msgid "Mongolian"
msgstr ""

#: ../../../inc/collections/_demo_content.funcs.php:2546
msgid ""
"vegetable oil\n"
"1/2 teaspoon ginger\n"
"1 tablespoon garlic\n"
"1/2 cup soy sauce\n"
"1/2 cup water\n"
"3/4 cup dark brown sugar\n"
"1 lb flank steak\n"
"1 yellow onion\n"
"2 large green onions"
